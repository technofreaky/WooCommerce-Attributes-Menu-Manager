{
  "name"        : "varunsridharan/wc-attributes-menu-manager",
  "description" : " Attributes Menu Manager For WooCommerce.",
  "readme"      : "readme.md",
  "type"        : "wordpress-plugin",
  "support"     : {
	"issues" : "https://github.com/varunsridharan/wc-attributes-menu-manager/issues"
  },
  "homepage"    : "https://github.com/varunsridharan/wc-attributes-menu-manager",
  "license"     : "GPL-3.0-or-later",
  "keywords"    : [
	"wordpress",
	"wordpress-plugin",
	"woocommerce",
	"woocommerce-plugin",
	"woocommerce-extension",
	"woocommerce-products",
	"woocommerce-product-sku",
	"woocommerce-sku",
	"woocommerce-shop",
	"woocommerce-seo",
	"wordpress-seo",
	"product-sku",
	"products",
	"product",
	"sku",
	"sku-link",
	"wc-product",
	"wc-product-sku"
  ],
  "require-dev" : {
	"varunsridharan/wp-cli-textdomain" : ">=1.1",
	"varunsridharan/wp2github-md"      : ">=1.1"
  },
  "scripts"     : {
	"makepot"        : "makepot ./wp-pot.json",
	"add-textdomain" : "add-textdomain -i \"wc-attrmm\" ./",
	"wp2githubmd"    : "wp2gitthub-md \"wc-attrmm\" \"./readme.txt\" ",
	"build"          : [ "@add-textdomain", "@makepot", "@wp2githubmd" ]
  },
  "authors"     : [
	{
	  "name"     : "Varun Sridharan",
	  "email"    : "varunsridharan23@gmail.com",
	  "homepage" : "http://varunsridharan.in",
	  "role"     : "Developer"
	}
  ]
}
